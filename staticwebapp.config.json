{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{jpg,gif,png}",
            "assets/*"
        ]
    },
    "globalHeaders": {
        "cache-control": "max-age=31536000;"
    },
    "routes": [
        {
            "route": "/",
            "headers": {
                "cache-control": "no-store"
            }
        }
    ]
}